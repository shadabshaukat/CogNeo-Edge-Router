# Router settings
ROUTER_REQUEST_TIMEOUT=30
ROUTER_UPSTREAM_TIMEOUT=30
TENANTS_CONFIG=tenants.yaml

# CORS
CORS_ENABLE=1
CORS_ALLOW_ORIGINS=*

# Metrics
METRICS_ENABLE=1

# Tenancy (optional): require X-Tenant-Id only when enabled
TENANCY_ENABLE=0

# Cache (Valkey/Redis) for response caching
CACHE_ENABLE=1
CACHE_TTL=60
CACHE_URL=redis://localhost:6379/0
